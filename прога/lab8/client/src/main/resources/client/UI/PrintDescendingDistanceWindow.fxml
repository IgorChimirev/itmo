<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Label?>

<AnchorPane fx:id="rootPane" prefHeight="150.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.UI.PrintDescendingDistanceWindowController">

    <!-- Заголовок -->
    <Label layoutX="20.0" layoutY="20.0" text="%print_desc_distance_title">
        <font>
            <Font size="14.0" />
        </font>
    </Label>

    <!-- Кнопка отправки -->
    <Button fx:id="submitButton" layoutX="110.0" layoutY="70.0"
            mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0"
            text="%submit" />

</AnchorPane>