<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="removeKeyPane" prefHeight="150.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/21"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.UI.RemoveKeyWindowController">

    <Label layoutX="20.0" layoutY="20.0" text="Введите ключ элемента:"/>
    <TextField fx:id="keyField" layoutX="20.0" layoutY="50.0" prefWidth="260.0"/>
    <Button fx:id="submitButton" layoutX="110.0" layoutY="90.0" text="Удалить"/>
    <Label fx:id="messageLabel" layoutX="20.0" layoutY="120.0" prefWidth="260.0" textFill="#ff0000"/>

</AnchorPane>